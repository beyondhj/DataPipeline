{
  "jobType": "batch",
  "version": "1",
  "setting": {
    "appName": "DatabaseTest",
    "spark.sql.shuffle.partitions": 200
  },
  "nodes": [
    {
      "id": "1",
      "name": "输入",
      "type": "reader",
      "config": {
        "format": "jdbc",
        "driver": "com.mysql.jdbc.Driver",
        "url": "jdbc:mysql://localhost/test?serverTimezone=UTC",
        "dbtable": "t_order0",
        "user": "root",
        "password": "root"
      }
    },
    {
      "id": "2",
      "name": "过滤",
      "type": "filter",
      "config": {
        "filter": "id > 0 and price > 1"
      }
    },
    {
      "id": "3",
      "name": "输出",
      "type": "writer",
      "format": "jdbc",
      "config": {
        "driver": "com.mysql.jdbc.Driver",
        "url": "jdbc:mysql://localhost/test?serverTimezone=UTC",
        "dbtable": "t_order1",
        "user": "root",
        "password": "root"
      }
    }
  ],
  "edges": [
    {
      "source": "1",
      "target": "2"
    },
    {
      "source": "2",
      "target": "3"
    }
  ]
}